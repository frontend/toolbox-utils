    "babel-loader": "^8.0.5",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.26.5",
    "chalk": "^2.4.2",
    "child_process": "^1.0.2",
    "cross-spawn": "^6.0.5",
    "cssnano": "^4.1.10",
    "del": "^4.1.1",
    "download": "^7.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
